        "anchor_from": "top_middle",
        "anchor_to": "top_middle",
        "offset": [ 0, 1 ],
        "$scroll_size": [ 5, "100%" ],
        "$scrolling_content": "bb_popup_dialog.custom_long_modal_dialog_scrolling_desc",
        "$scroll_bar_right_padding_size": [ 4, 0 ],
        "$scroll_track_offset": [ -2, 0 ],
        "clips_children": true,
        "$show_background": false
