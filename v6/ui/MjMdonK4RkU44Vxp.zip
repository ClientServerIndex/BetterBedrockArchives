    "crosshair_button@bb.p":
    {
        "$blank|default": false,
        "$default_selector_toggle_index|default": 0,
        "$is_checked_state|default": false,
        "$is_hover_state|default": false,
        "$tab_panel": "bb_mod_crosshair.tab_panel",
        "$tab_view_binding_name|default": "hbui_tab",
        "$toggle_button_mappings_control|default": [
        {
            "from_button_id": "button.menu_select",
            "mapping_type": "pressed",
            "to_button_id": "button.menu_select"
        },
        {
            "from_button_id": "button.menu_ok",
            "mapping_type": "focused",
            "to_button_id": "button.menu_ok"
        },
        {
            "from_button_id": "button.controller_back",
            "ignored": "(not $allow_controller_back_button_mapping)",
            "mapping_type": "focused",
            "to_button_id": "button.controller_back"
        },
        {
            "from_button_id": "button.menu_left",
            "ignored": "(not $enable_directional_toggling)",
            "input_mode_condition": "gamepad_and_not_gaze",
            "mapping_type": "pressed",
            "to_button_id": "$toggle_off_button"
        },
        {
            "from_button_id": "button.menu_right",
            "ignored": "(not $enable_directional_toggling)",
            "input_mode_condition": "gamepad_and_not_gaze",
            "mapping_type": "pressed",
            "to_button_id": "$toggle_on_button"
        } ],
        "$toggle_group_default_selected": "$default_selector_toggle_index",
        "$toggle_name|default": "hbui_tab",
        "controls": [
        {
            "$tab_view_binding_name@common.toggle2":
            {
                "$toggle_button_mappings": "$toggle_button_mappings_control",
                "controls": [
                {
                    "unchecked@$tab_panel":
                    {
                        "$show_crosshairs": true,
                        "layer": 1
                    }
                },
                {
                    "unchecked_hover@$tab_panel":
                    {
                        "$visible_hover_panel": true,
                        "layer": 1
                    }
                },
                {
                    "checked@$tab_panel":
                    {
                        "$show_crosshairs": true,
                        "layer": 2
                    }
                },
                {
                    "checked_hover@$tab_panel":
                    {
                        "$visible_hover_panel": true,
                        "layer": 2
                    }
                } ],
                "reset_on_focus_lost": false
            }
        } ],
        "size": [ "100%", 32 ]
    },
