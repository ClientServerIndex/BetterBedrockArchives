    "mod_panel@bb.p":
    {
        "controls": [
        {
            "@bb.p":
            {
                "controls": [
                {
                    "button_texture@bb.img":
                    {
                        "ignored": "(not $use_button_texture",
                        "layer": 1,
                        "size": "$texture_size",
                        "texture": "$button_texture"
                    }
                },
                {
                    "button_text@bb.l":
                    {
                        "color": "$button_text_color",
                        "layer": 2,
                        "offset": "$button_text_offset",
                        "shadow": false,
                        "text": "$button_text"
                    }
                },
                {
                    "hover_panel@common.hover_text":
                    {
                        "$hover_text|default": "",
                        "allow_clipping": false,
                        "bindings": [
                        {
                            "binding_type": "view",
                            "source_property_name": "#hover_text_control",
                            "target_property_name": "#hover_text"
                        } ],
                        "layer": 1000000,
                        "property_bag":
                        {
                            "#hover_text_control": "$hover_text"
                        },
                        "size": [ "100%", "100%" ],
                        "visible": "$is_hover_state"
                    }
                },
                {
                    "custom_content@$custom_content":
                    {
                        "ignored": "(not $use_custom_content)",
                        "layer": 3
                    }
                } ],
                "visible": "(not $blank)"
            }
        } ]
    },
    "mod_settings_panel@bb.p":
    {
        "$settings_content|default": "common_tabs.empty_tab_content",
        "controls": [
        {
            "button_texture@bb.img":
            {
                "$ignore_gear_texture|default": false,
                "$settings_texture|default": "AMBRO/tui/common/empty",
                "layer": 1,
                "size": [ 16, 16 ],
                "texture": "$settings_texture",
                "visible": "(not $ignore_gear_texture)"
            }
        },
        {
            "settings_content@$settings_content":
            {
                "bindings": [
                {
                    "binding_type": "view",
                    "source_control_name": "bb_button_gameplay",
                    "source_property_name": "#toggle_state",
                    "target_property_name": "#visible"
                } ],
                "layer": 2
            }
        } ]
    },
    "mod_settings@bb.p":
    {
        "$button_panel": "bb_hbui_buttons.mod_settings_panel",
        "$checked_default_image": "AMBRO/tui/hbui/more/option_default",
        "$checked_hover_image": "AMBRO/tui/hbui/more/option_default",
        "$is_checked_state|default": false,
        "$is_hover_state|default": false,
        "$radio_toggle_group": true,
        "$settings_binding_name|default": "",
        "$settings_toggle_index|default": -1,
        "$tab_view_binding_name": "$settings_binding_name",
        "$toggle_button_mappings_control|default": [
        {
            "from_button_id": "button.menu_select",
            "mapping_type": "pressed",
            "to_button_id": "button.menu_select"
        },
        {
            "from_button_id": "button.menu_ok",
            "mapping_type": "focused",
            "to_button_id": "button.menu_ok"
        } ],
        "$toggle_group_default_selected": 0,
        "$toggle_group_forced_index": "$settings_toggle_index",
        "$toggle_name": "hbui_mod_settings",
        "$unchecked_default_image": "AMBRO/tui/hbui/more/option_default",
        "$unchecked_hover_image": "AMBRO/tui/hbui/more/option_hover",
        "$use_settings|default": false,
        "anchor_from": "top_right",
        "anchor_to": "top_right",
        "controls": [
        {
            "$tab_view_binding_name@common.toggle2":
            {
                "$toggle_button_mappings": "$toggle_button_mappings_control",
                "controls": [
                {
                    "unchecked@$button_panel":
                    {
                        "$settings_close_button": "AMBRO/tui/common/empty",
                        "$settings_texture": "$unchecked_default_image",
                        "$tab_state": "unchecked",
                        "layer": 1
                    }
                },
                {
                    "unchecked_hover@$button_panel":
                    {
                        "$is_hover_state": true,
                        "$settings_close_button": "AMBRO/tui/buttons/close/hover_no_icon",
                        "$settings_texture": "$unchecked_hover_image",
                        "$tab_state": "unchecked_hover",
                        "layer": 1
                    }
                },
                {
                    "checked@$button_panel":
                    {
                        "$is_checked_state": true,
                        "$tab_state": "checked",
                        "layer": 2
                    }
                },
                {
                    "checked_hover@$button_panel":
                    {
                        "$is_checked_state": true,
                        "$is_hover_state": true,
                        "$tab_state": "checked_hover",
                        "layer": 2
                    }
                } ],
                "reset_on_focus_lost": false
            }
        } ],
        "layer": 100,
        "size": [ 23, 23 ],
        "visible": "$use_settings"
    },
