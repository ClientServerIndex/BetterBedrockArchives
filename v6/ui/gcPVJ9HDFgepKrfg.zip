                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "layer": 2,
                "offset": [ 18, 0 ],
                "shadow": false,
                "size": [ "100% - 18px", 10 ],
                "text": "$checkbox_text"
            }
        } ]
    },
    "checkbox@bb.p":
    {
        "$checkbox_panel": "bb_hbui_buttons.checkbox_panel",
        "$checkbox_size|default": [ "100%", 16 ],
        "$checkbox_state|default": false,
        "$checked_default_image": "AMBRO/tui/hbui/checkbox/pressed",
        "$checked_hover_image": "AMBRO/tui/hbui/checkbox/pressed_hover",
        "$checked_hover_text_color|default": [ 1, 1, 1 ],
        "$checked_text_color|default": [ 1, 1, 1 ],
        "$default_selector_toggle_index|default": 0,
        "$is_checked_state|default": false,
        "$is_hover_state|default": false,
