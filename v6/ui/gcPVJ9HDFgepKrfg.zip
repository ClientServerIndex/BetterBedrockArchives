        "$unchecked_text_color|default": [ 0.1176470588235294, 0.1176470588235294, 0.1176470588235294 ],
        "controls": [
        {
            "$tab_view_binding_name@common.toggle":
            {
                "controls": [
                {
                    "unchecked@$checkbox_panel":
                    {
                        "$checkbox_texture": "$unchecked_default_image",
                        "$tab_state": "unchecked",
                        "layer": 1
                    }
                },
                {
                    "unchecked_hover@$checkbox_panel":
                    {
                        "$checkbox_texture": "$unchecked_hover_image",
                        "$is_hover_state": true,
                        "$tab_state": "unchecked_hover",
                        "layer": 1
                    }
                },
                {
                    "checked@$checkbox_panel":
                    {
                        "$checkbox_texture": "$checked_default_image",
                        "$is_checked_state": true,
                        "$tab_state": "checked",
                        "layer": 2
                    }
                },
                {
                    "checked_hover@$checkbox_panel":
                    {
                        "$checkbox_texture": "$checked_hover_image",
                        "$is_checked_state": true,
                        "$is_hover_state": true,
                        "$tab_state": "checked_hover",
                        "layer": 2
                    }
                } ],
                "reset_on_focus_lost": false
            }
        } ],
        "size": "$checkbox_size"
    },
