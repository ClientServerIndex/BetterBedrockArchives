{
    "namespace": "bb_content_log",
    "content_log_history_main_panel@bb.p":
    {
        "controls": [
        {
            "@bb_common.screen_header":
            {
                "$screen_header_text": "Content Log"
            }
        },
        {
            "@bb.p":
            {
                "layer": 2,
                "anchor_from": "bottom_middle",
                "anchor_to": "bottom_middle",
                "size": [ "100%", "100% - 28px" ],
                "controls": [
                {
                    "@bb.img":
                    {
                        "texture": "AMBRO/tui/common/black",
                        "alpha": 0.6,
                        "controls": [
                        {
                            "@bb_content_log.main_stack_panel":
                            {
                                "size": [ "100% - 4px", "100% - 2px" ]
                            }
                        } ]
                    }
                } ]
            }
        } ]
    },
    "main_stack_panel@bb.sp":
    {
        "size": [ "100%", "100%" ],
        "controls": [
        {
            "messages_scrolling_panel@common.scrolling_panel":
            {
                "type": "panel",
                "size": [ "100%", "100%" ],
                "anchor_from": "bottom_left",
                "anchor_to": "bottom_left",
                "offset": [ 2, -27 ],
                "$scrolling_content": "bb_content_log.messages_stack_panel",
                "$scroll_size": [ 5, "100%-4px" ],
                "$jump_to_bottom_on_update": true,
                "$show_background": false,
                "$always_handle_scrolling": true
            }
        } ]
    },
    "messages_stack_panel@bb.sp":
    {
        "$scrolling_content_anchor_from": "top_left",
        "$scrolling_content_anchor_to": "top_left",
        "collection_name": "content_log_message",
        "size": [ "100%", "default" ],
        "factory":
        {
            "name": "messages_factory",
            "control_name": "content_log_message@bb_content_log.content_log_message_panel"
        },
        "bindings": [
        {
            "binding_name": "#messages_size",
            "binding_type": "global",
            "binding_name_override": "#collection_length"
        } ]
    },
    "content_log_message_panel@bb.p":
    {
        "size": [ "100%", "100%c" ],
        "controls": [
        {
            "content_log_message@bb.label":
            {
                "color": "$chat_text_color",
                "localize": false,
                "size": [ "100%", "default" ],
                "text": "#text",
                "font_type": "smooth",
                "line_padding": 1,
                "font_scale_factor": 1,
                "bindings": [
                {
                    "binding_type": "collection",
                    "binding_name": "#content_log_text",
                    "binding_name_override": "#text",
                    "binding_collection_name": "content_log_message"
                } ]
            }
        } ]
    },
    "clipboard_button@common_buttons.light_content_button":
    {
        "size": [ 30, 30 ],
        "$pressed_button_name": "button.copy_to_clipboard",
        "$button_content": "content_log_history.clipboard_icon_wrapper"
    },
    "clipboard_icon_wrapper":
    {
        "type": "panel",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "size": [ "100% - 2px", "100% - 2px" ],
        "controls": [
        {
            "stack_panel":
            {
                "type": "stack_panel",
                "orientation": "horizontal",
                "controls": [
                {
                    "icon_panel":
                    {
                        "type": "panel",
                        "size": [ "100%c", "100%" ],
                        "controls": [
                        {
                            "clipboard_icon":
                            {
                                "layer": 1,
                                "type": "image",
                                "texture": "textures/ui/copy",
                                "size": [ 13, 14 ]
                            }
                        } ]
                    }
                },
                {
                    "offset":
                    {
                        "type": "panel",
                        "size": [ 4, "100%" ]
                    }
                },
                {
                    "label_panel":
                    {
                        "type": "panel",
                        "size": [ "100%c", "100%" ],
                        "controls": [
                        {
                            "label":
                            {
                                "type": "label",
                                "text": "contentlog.history.copyToClipboard",
                                "color": "$text_color"
                            }
                        } ]
                    }
                } ]
            }
        } ]
    },
    "content_log_message_panel_panel":
    {
        "type": "panel",
        "size": [ "100%", "100%cm" ],
        "controls": [
        {
            "content_log_message_panel@content_log_history.content_log_message_panel":
            {}
        } ]
    }
}