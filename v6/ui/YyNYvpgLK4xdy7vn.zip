                        "texture": "AMBRO/tui/popup_images/changelog/1"
                    }
                },
                {
                    "2@bb.img":
                    {
                        "texture": "AMBRO/tui/popup_images/changelog/2"
                    }
                },
                {
                    "3@bb.img":
                    {
                        "texture": "AMBRO/tui/popup_images/changelog/3"
                    }
                },
                {
                    "4@bb.img":
                    {
                        "texture": "AMBRO/tui/popup_images/changelog/4"
                    }
                },
                {
                    "1x@bb.img":
                    {
                        "texture": "AMBRO/tui/popup_images/changelog/1"
                    }
                } ]
            }
        },
        {
            "sm@bb.p":
            {}
        },
        {
            "theme":
            {
                "layer": 10,
                "size": [ "100%", "100%" ],
                "type": "custom",
                "renderer": "gradient_renderer",
                "color1": [ 0.4549019607843137, 0.6313725490196078, 0.9568627450980393 ],
                "color2": [ 0.4549019607843137, 0.6313725490196078, 0.9568627450980393 ]
            }
        } ]
    },
