{
    "namespace": "bb_start",
    "start_screen_main_panel@bb.p":
    {
        "$achievements_ignored|default": false,
        "$feedback_ignored|default": false,
        "$store_ignored|default": false,
        "controls": [
        {
            "@bb_start.title":
            {}
        },
        {
            "@bb_start.main_buttons":
            {}
        },
        {
            "@bb_start.copyright_panel":
            {}
        },
        {
            "@bb_start.header":
            {}
        },
        {
            "popup_dialog_factory":
            {
                "type": "factory",
                "control_ids":
                {
                    "cloud_upload_prompt": "@start.cloud_upload_prompt"
                }
            }
        },
        {
            "popups@bb.p":
            {
                "layer": 10,
                "controls": [
                {
                    "@bb_start.changelog_popup":
                    {}
                },
                {
                    "@bb_start.app_popup":
                    {}
                },
                {
                    "bg@bb_common.screen_background":
                    {
                        "layer": 10
                    }
                },
                {
                    "lock_all_buttons_under_popup":
                    {
                        "type": "button",
                        "layer": 9
                    }
                } ],
                "bindings": [
                {
                    "binding_type": "view",
                    "source_control_name": "show_changelog_popup",
                    "source_property_name": "#toggle_state",
                    "target_property_name": "#condition1"
                },
                {
                    "binding_type": "view",
                    "source_control_name": "show_app_popup",
                    "source_property_name": "#toggle_state",
                    "target_property_name": "#condition2"
                },
                {
                    "binding_type": "view",
                    "source_property_name": "(#condition1 or #condition2)",
                    "target_property_name": "#visible"
                } ]
            }
        } ]
    },
    "header@bb.p":
    {
        "anchor_from": "top_middle",
        "anchor_to": "top_middle",
        "size": [ "100%", 28 ],
        "controls": [
        {
            "@bb.img":
            {
                "texture": "AMBRO/tui/header",
                "controls": [
                {
                    "right_side@bb.sph":
                    {
                        "anchor_from": "right_middle",
                        "anchor_to": "right_middle",
                        "offset": [ -1, 1 ],
                        "controls": [
                        {
                            "feedback_button@bb_hbui_buttons.pressed2":
                            {
                                "$focus_override_up": "FOCUS_OVERRIDE_STOP",
                                "$default_button": "AMBRO/tui/common/empty",
                                "$hover_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button_name": "button.menu_open_uri",
                                "$button_text": "F",
                                "$hover_text": "Open Feedback Website §c(Press 2x)",
                                "size": [ 24, 24 ],
                                "property_bag":
                                {
                                    "#hyperlink": "https://aka.ms/minecraftfb"
                                },
                                "button_mappings": [
                                {
                                    "from_button_id": "button.menu_select",
                                    "to_button_id": "$pressed_button_name",
                                    "mapping_type": "double_pressed"
                                },
                                {
                                    "from_button_id": "button.menu_ok",
                                    "to_button_id": "$pressed_button_name",
                                    "mapping_type": "focused"
                                } ]
                            }
                        },
                        {
                            "realms_button@bb_hbui_buttons.pressed2":
                            {
                                "$focus_override_up": "FOCUS_OVERRIDE_STOP",
                                "$default_button": "AMBRO/tui/common/empty",
                                "$hover_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button_name": "button.menu_invite_notification",
                                "$button_text": "R",
                                "$hover_text": "Open Realms Notifications",
                                "size": [ 24, 24 ]
                            }
                        },
                        {
                            "marketplace_button@bb_hbui_buttons.pressed2":
                            {
                                "$focus_override_up": "FOCUS_OVERRIDE_STOP",
                                "$default_button": "AMBRO/tui/common/empty",
                                "$hover_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button_name": "button.menu_store",
                                "$button_text": "M",
                                "$hover_text": "Open Marketplace",
                                "size": [ 24, 24 ]
                            }
                        },
                        {
                            "achievements_button@bb_hbui_buttons.pressed2":
                            {
                                "$focus_override_up": "FOCUS_OVERRIDE_STOP",
                                "$default_button": "AMBRO/tui/common/empty",
                                "$hover_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button_name": "button.menu_achievements",
                                "$button_text": "A",
                                "$hover_text": "Open Achievements",
                                "size": [ 24, 24 ]
                            }
                        },
                        {
                            "dressing_room_button@bb_hbui_buttons.pressed2":
                            {
                                "$focus_override_up": "FOCUS_OVERRIDE_STOP",
                                "$default_button": "AMBRO/tui/common/empty",
                                "$hover_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button_name": "button.to_profile_screen",
                                "$button_text": "D",
                                "$hover_text": "Open Dressing Room",
                                "size": [ 24, 24 ]
                            }
                        },
                        {
                            "close_button_mapping@bb_hbui_buttons.pressed2":
                            {
                                "$focus_id": "close_button_mapping",
                                "$focus_override_up": "FOCUS_OVERRIDE_STOP",
                                "$focus_override_right": "FOCUS_OVERRIDE_STOP",
                                "$use_custom_content": true,
                                "$default_button": "AMBRO/tui/common/empty",
                                "$hover_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button_name": "button.menu_exit",
                                "$hover_text": "Close Minecraft",
                                "$custom_content": "bb_start.close_button_image_content",
                                "size": [ 24, 24 ],
                                "button_mappings": [
                                {
                                    "from_button_id": "button.menu_cancel",
                                    "to_button_id": "button.menu_exit",
                                    "mapping_type": "global"
                                },
                                {
                                    "from_button_id": "button.menu_select",
                                    "to_button_id": "$pressed_button_name",
                                    "mapping_type": "pressed"
                                },
                                {
                                    "from_button_id": "button.menu_ok",
                                    "to_button_id": "$pressed_button_name",
                                    "mapping_type": "focused"
                                } ],
                                "bindings": [
                                {
                                    "binding_type": "view",
                                    "source_control_name": "show_changelog_popup",
                                    "source_property_name": "(not #toggle_state)",
                                    "target_property_name": "#condition1"
                                },
                                {
                                    "binding_type": "view",
                                    "source_control_name": "show_app_popup",
                                    "source_property_name": "(not #toggle_state)",
                                    "target_property_name": "#condition2"
                                },
                                {
                                    "binding_type": "view",
                                    "source_property_name": "((#condition1 and #condition2) or (#condition2 and #condition1))",
                                    "target_property_name": "#visible"
                                } ]
                            }
                        },
                        {
                            "close_button_mapping@bb_hbui_buttons.pressed2":
                            {
                                "$focus_id": "close_button_mapping",
                                "$focus_override_up": "FOCUS_OVERRIDE_STOP",
                                "$focus_override_right": "FOCUS_OVERRIDE_STOP",
                                "$use_custom_content": true,
                                "$default_button": "AMBRO/tui/common/empty",
                                "$hover_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button": "AMBRO/tui/buttons/close/hover_no_icon",
                                "$pressed_button_name": "button.menu_exit",
                                "$hover_text": "Close Minecraft",
                                "$custom_content": "bb_start.close_button_image_content",
                                "size": [ 24, 24 ],
                                "bindings": [
                                {
                                    "binding_type": "view",
                                    "source_control_name": "show_changelog_popup",
                                    "source_property_name": "#toggle_state",
                                    "target_property_name": "#condition1"
                                },
                                {
                                    "binding_type": "view",
                                    "source_control_name": "show_app_popup",
                                    "source_property_name": "#toggle_state",
                                    "target_property_name": "#condition2"
                                },
                                {
                                    "binding_type": "view",
                                    "source_property_name": "(#condition1 or #condition2)",
                                    "target_property_name": "#visible"
                                } ]
                            }
                        } ]
                    }
                },
                {
                    "left_side@bb.sph":
                    {
                        "anchor_from": "left_middle",
                        "anchor_to": "left_middle",
                        "offset": [ 3, 2 ],
                        "controls": [
                        {
                            "gamerpic@bb.img":
                            {
                                "size": [ 22, 22 ],
                                "bindings": [
                                {
                                    "binding_name": "#gamerpic_texture_path",
                                    "binding_name_override": "#texture"
                                },
                                {
                                    "binding_name": "#gamerpic_texture_location_type",
                                    "binding_name_override": "#texture_file_system"
                                } ]
                            }
                        },
                        {
                            "@bb.yp":
                            {
                                "$yps": [ 3, 0 ]
                            }
                        },
                        {
                            "@bb.p":
                            {
                                "size": [ "100%cm", 22 ],
                                "controls": [
                                {
                                    "@bb.sp":
                                    {
                                        "offset": [ 0, 0.5 ],
                                        "controls": [
                                        {
                                            "@bb.l":
                                            {
                                                "text": "You're playing as:",
                                                "shadow": false,
                                                "color": [ 0.1176470588235294, 0.1176470588235294, 0.1176470588235294 ]
                                            }
                                        },
                                        {
                                            "@bb.l":
                                            {
                                                "text": "#playername",
                                                "shadow": false,
                                                "color": [ 0.1176470588235294, 0.1176470588235294, 0.1176470588235294 ],
                                                "bindings": [
                                                {
                                                    "binding_name": "#playername"
                                                } ]
                                            }
                                        } ]
                                    }
                                } ]
                            }
                        } ]
                    }
                },
                {
                    "bg":
                    {
                        "layer": -1,
                        "type": "custom",
                        "anchor_from": "top_middle",
                        "anchor_to": "top_middle",
                        "renderer": "gradient_renderer",
                        "size": [ "100%", "200%" ],
                        "color1": [ 0, 0, 0, 0.55 ],
                        "color2": [ 0.1176470588235294, 0.1176470588235294, 0.1176470588235294, 0 ]
                    }
                } ]
            }
        } ]
    },
    "dressing_room_image_content@bb.img":
    {
        "texture": "textures/ui/dressing_room_capes",
        "size": [ 16, 16 ]
    },
    "close_button_image_content@bb.img":
    {
        "size": [ "100% - 4px", "100% - 4px" ],
        "texture": "AMBRO/tui/buttons/close/exit"
    },
    "copyright_panel@bb.p":
    {
        "controls": [
        {
            "left_side@bb.pxyc":
            {
                "anchor_from": "bottom_left",
                "anchor_to": "bottom_left",
                "offset": [ 2, -2 ],
                "controls": [
                {
                    "@bb.sp":
                    {
                        "controls": [
                        {
                            "@bb_start.copyright_label_template":
                            {
                                "text": "Copyright © 2022-2023 Better Bedrock. All rights reserved"
                            }
                        } ]
                    }
                } ]
            }
        },
        {
            "right_side@bb.pxyc":
            {
                "anchor_from": "bottom_right",
                "anchor_to": "bottom_right",
                "offset": [ -2, -2 ],
                "controls": [
                {
                    "@bb.sph":
                    {
                        "size": [ "100%c", 8 ],
                        "controls": [
                        {
                            "@bb_start.copyright_label_template":
                            {
                                "text": "Copyright Mojang AB. Do not distribute | "
                            }
                        },
                        {
                            "@bb_start.copyright_label_template":
                            {
                                "text": "#version",
                                "bindings": [
                                {
                                    "binding_name": "#version"
                                } ]
                            }
                        },
                        {
                            "@bb_start.copyright_label_template":
                            {
                                "text": " | v"
                            }
                        },
                        {
                            "@bb_start.copyright_label_template":
                            {
                                "text": "#development_version",
                                "bindings": [
                                {
                                    "binding_name": "#development_version"
                                } ]
                            }
                        } ]
                    }
                } ]
            }
        } ]
    },
    "copyright_label_template@bb.l":
    {
        "font_type": "smooth",
        "color": [ 0.8, 0.8, 0.8 ],
        "font_scale_factor": 0.8,
        "alpha": 0.6
    },
    "main_buttons@bb.p":
    {
        "$yps": [ 0, 2 ],
        "size": [ 220, "100%" ],
        "controls": [
        {
            "@bb.sp":
            {
                "size": [ "100%", "100%c" ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "controls": [
                {
                    "@bb.yp":
                    {
                        "$yps": [ 0, 70 ]
                    }
                },
                {
                    "@bb_start.main_button_template":
                    {
                        "$default_focus_precedence": 10,
                        "$pressed_text": "Play",
                        "$default_text_color": [ 1, 1, 1 ],
                        "$hover_text_color": [ 1, 1, 1 ],
                        "$default_button": "AMBRO/tui/hbui/button/green/button_default",
                        "$hover_button": "AMBRO/tui/hbui/button/green/button_hover",
                        "$pressed_button": "AMBRO/tui/hbui/button/green/button_pressed",
                        "size": [ "100%", 36 ],
                        "$font_scale_factor": 1.5,
                        "$pressed_button_name": "$play_button_target"
                    }
                },
                {
                    "@bb.yp":
                    {}
                },
                {
                    "@bb_start.main_button_template":
                    {
                        "$panel_offset": [ 0, 0 ],
                        "$font_type": "default",
                        "$pressed_text": "Settings",
                        "$pressed_button_name": "button.menu_settings"
                    }
                },
                {
                    "@bb.yp":
                    {}
                },
                {
                    "@bb_hbui_buttons.tab":
                    {
                        "$radio_toggle_group": true,
                        "$toggle_group_forced_index": 1,
                        "$default_selector_toggle_index": 0,
                        "$tab_view_binding_name": "show_changelog_popup",
                        "$toggle_name": "bb_changelog_tab",
                        "$unchecked_default_image": "AMBRO/tui/hbui/button_default",
                        "$unchecked_hover_image": "AMBRO/tui/hbui/button_hover",
                        "$checked_default_image": "AMBRO/tui/hbui/button_default",
                        "$checked_hover_image": "AMBRO/tui/hbui/button_hover",
                        "size": [ "100%", 24 ],
                        "$tab_texture_size": [ "100%", "100% " ],
                        "$use_tab_content": true,
                        "$tab_text": "Changelog",
                        "clips_children": true
                    }
                },
                {
                    "@bb.yp":
                    {}
                },
                {
                    "@bb_hbui_buttons.tab":
                    {
                        "$radio_toggle_group": true,
                        "$toggle_group_forced_index": 1,
                        "$default_selector_toggle_index": 0,
                        "$tab_view_binding_name": "show_app_popup",
                        "$toggle_name": "bb_app_tab",
                        "$unchecked_default_image": "AMBRO/tui/hbui/button_default",
                        "$unchecked_hover_image": "AMBRO/tui/hbui/button_hover",
                        "$checked_default_image": "AMBRO/tui/hbui/button_default",
                        "$checked_hover_image": "AMBRO/tui/hbui/button_hover",
                        "size": [ "100%", 24 ],
                        "$tab_texture_size": [ "100%", "100% " ],
                        "$use_tab_content": true,
                        "$tab_content": "bb_start.app_tab_content",
                        "$tab_text": "",
                        "clips_children": true
                    }
                } ]
            }
        } ]
    },
    "main_button_template@bb_start.pressed":
    {
        "size": [ "100%", 24 ]
    },
    "popup_close_button_mapping@bb_hbui_buttons.tab":
    {
        "layer": 21,
        "$blank": true,
        "$toggle_group_forced_index": 4,
        "$radio_toggle_group": true,
        "$default_selector_toggle_index": 0,
        "$tab_text": "",
        "size": [ "100%", "100%" ],
        "$toggle_button_mappings_control": [
        {
            "from_button_id": "button.menu_cancel",
            "to_button_id": "button.menu_select",
            "mapping_type": "pressed"
        },
        {
            "from_button_id": "button.menu_cancel",
            "to_button_id": "button.menu_ok",
            "mapping_type": "focused"
        } ]
    },
    "changelog_popup@bb.p":
    {
        "controls": [
        {
            "@bb_start.popup_close_button_mapping":
            {
                "$toggle_name": "bb_changelog_tab"
            }
        },
        {
            "popup@bb_popup_dialog.custom_long_modal_dialog":
            {
                "$custom_modal_dialog_title_content": "bb_start.custom_modal_dialog_title_changelog_content",
                "$custom_modal_dialog_desc_img_content": "bb_start.custom_modal_dialog_desc_img_changelog_content",
                "$custom_modal_dialog_desc_content": "bb_start.custom_modal_dialog_desc_changelog_content",
                "$button_panel": "bb_start.changelog_button_panel"
            }
        } ],
        "bindings": [
        {
            "binding_type": "view",
            "source_control_name": "show_changelog_popup",
            "source_property_name": "#toggle_state",
            "target_property_name": "#visible"
        } ]
    },
    "custom_modal_dialog_title_changelog_content@bb.p":
    {
        "size": [ "100%", 10 ],
        "clips_children": true,
        "$transition_duration": 0.75,
        "$wait_duration": 4,
        "$easing_in": "out_cubic",
        "$easing_out": "out_cubic",
        "controls": [
        {
            "@bb.l":
            {
                "text": "Changelog",
                "shadow": false
            }
        } ]
    },
