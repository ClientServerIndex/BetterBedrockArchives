        {
            "$toggle_button_mappings_control": [
            {
                "from_button_id": "button.menu_select",
                "mapping_type": "pressed",
                "to_button_id": "button.menu_ok"
            },
            {
                "from_button_id": "button.menu_ok",
                "mapping_type": "pressed",
                "to_button_id": "button.menu_ok"
            },
            {
                "from_button_id": "button.menu_cancel",
                "mapping_type": "global",
                "to_button_id": "button.menu_ok"
            },
            {
                "from_button_id": "button.menu_exit",
                "mapping_type": "global",
                "to_button_id": "button.menu_ok"
            } ],
            "requires": "($subpack = '1')"
        },
        {
            "$toggle_button_mappings_control": [
            {
                "from_button_id": "button.menu_select",
                "ignored": "$game_pad",
                "mapping_type": "pressed",
                "to_button_id": "button.menu_ok"
            },
            {
                "from_button_id": "button.menu_ok",
                "ignored": "$game_pad",
                "mapping_type": "pressed",
                "to_button_id": "button.menu_ok"
            },
            {
                "from_button_id": "button.menu_cancel",
                "mapping_type": "global",
                "to_button_id": "button.menu_ok"
            },
            {
                "from_button_id": "button.menu_exit",
                "mapping_type": "global",
                "to_button_id": "button.menu_ok"
            } ],
            "requires": "($subpack = '2')"
        } ]
    },
