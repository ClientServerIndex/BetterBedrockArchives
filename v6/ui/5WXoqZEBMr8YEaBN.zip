{
    "namespace": "bb_progress",
    "progress_screen_main_panel@bb.p":
    {
        "controls": [
        {
            "@bb_start.title":
            {}
        },
        {
            "@bb_progress.progress_bar_percentage":
            {}
        },
        {
            "scan_anim@bb.p":
            {
                "size": [ "100%", 10 ],
                "offset": [ 0, 20 ],
                "clips_children": true,
                "controls": [
                {
                    "@bb.l":
                    {
                        "text": "0 o o",
                        "alpha": 0,
                        "shadow": false,
                        "anims": [ "@progress.anim1_1" ]
                    }
                },
                {
                    "@bb.l":
                    {
                        "text": "o 0 o",
                        "alpha": 0,
                        "shadow": false,
                        "anims": [ "@progress.anim2_1" ]
                    }
                },
                {
                    "@bb.l":
                    {
                        "text": "o o 0",
                        "alpha": 0,
                        "shadow": false,
                        "anims": [ "@progress.anim3_1" ]
                    }
                } ]
            }
        },
        {
            "main_sp@bb.sp":
            {
                "anchor_from": "bottom_middle",
                "anchor_to": "bottom_middle",
                "controls": [
                {
                    "progress_title_text@bb.pyc":
                    {
                        "controls": [
                        {
                            "cancel_button@bb_hbui_buttons.pressed":
                            {
                                "$pressed_button_name": "button.menu_exit",
                                "$pressed_text": "Cancel",
                                "size": [ 100, 32 ],
                                "bindings": [
                                {
                                    "binding_name": "#cancel_visible",
                                    "binding_name_override": "#visible"
                                } ]
                            }
                        } ]
                    }
                },
                {
                    "@bb.yp":
                    {
                        "$yps": [ 0, 3 ]
                    }
                },
                {
                    "progress_title_text@bb.pyc":
                    {
                        "controls": [
                        {
                            "@bb.l":
                            {
                                "text": "#title_text",
                                "shadow": false,
                                "bindings": [
                                {
                                    "binding_name": "#title_text"
                                } ]
                            }
                        } ]
                    }
                },
                {
                    "progress_bar_text@bb.pyc":
                    {
                        "controls": [
                        {
                            "@bb.l":
                            {
                                "size": [ "100% - 66px", 10 ],
                                "text": "#progress_text",
                                "text_alignment": "center",
                                "shadow": false,
                                "bindings": [
                                {
                                    "binding_name": "#progress_text"
                                },
                                {
                                    "binding_type": "view",
                                    "source_property_name": "(not (#progress_text = ''))",
                                    "target_property_name": "#visible"
                                } ]
                            }
                        } ]
                    }
                },
                {
                    "@bb.yp":
                    {
                        "$yps": [ 0, 3 ]
                    }
                },
                {
                    "progress_bar@bb.pyc":
                    {
                        "controls": [
                        {
                            "@bb.p":
                            {
                                "size": [ "100% - 18px", 5 ],
                                "controls": [
                                {
                                    "full_progress_bar_icon@achievement.full_progress_bar_icon_base":
                                    {
                                        "layer": 2,
                                        "bindings": [
                                        {
                                            "binding_name": "#loading_bar_percentage",
                                            "binding_name_override": "#clip_ratio"
                                        } ]
                                    }
                                },
                                {
                                    "empty_progress_bar_icon@achievement.empty_progress_bar_icon":
                                    {
                                        "layer": 1
                                    }
                                },
                                {
                                    "progress_bar_nub@progress.progress_bar_nub":
                                    {
                                        "size": [ "100% - 10px", 5 ],
                                        "layer": 4
                                    }
                                } ]
                            }
                        } ]
                    }
                },
                {
                    "@bb.yp":
                    {
                        "$yps": [ 0, 9 ]
                    }
                } ]
            }
        },
        {
            "popup_dialog_factory@progress.popup_dialog_factory":
            {}
        } ]
    },
    "progress_bar_percentage_control@bb.label":
    {
        "text": "('Â§z' + $text + '%')",
        "localize": false,
        "shadow": false,
        "bindings": [
        {
            "binding_name": "#loading_bar_percentage"
        },
        {
            "binding_type": "view",
            "source_property_name": "(((1 - #loading_bar_percentage) * 100 > ($in_range - 1)) and ((1 - #loading_bar_percentage) * 100 < $in_range) or ((1 - #loading_bar_percentage) * 100 = $in_range))",
            "target_property_name": "#visible"
        } ]
    },
